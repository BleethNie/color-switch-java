<VBox prefHeight="800.0" prefWidth="500.0" style="-fx-background-color: #222;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SavedGamesController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Text fill="WHITE" layoutX="96.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SAVED GAMES">
               <font>
                  <Font name="Blissful Thinking" size="52.0" />
               </font>
            </Text>
            <ScrollPane fx:id="scrollPane" layoutY="168.0" prefHeight="596.0" prefWidth="500.0">
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
               <content>
                  <VBox alignment="CENTER" spacing="25.0" />
               </content>
            </ScrollPane>
            <Group fx:id="backBtn" layoutX="-198.0" layoutY="-63.0" onMouseClicked="#backBtnClicked" scaleX="0.5" scaleY="0.5" styleClass="clickable">
               <children>
                  <Circle fill="#666666" layoutX="237.0" layoutY="102.0" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
                  <Text fill="WHITE" fontSmoothingType="LCD" layoutX="211.0" layoutY="115.0" stroke="WHITE" strokeWidth="3.0" text="←">
                     <font>
                        <Font name="Arial Bold" size="54.0" />
                     </font>
                  </Text>
               </children>
            </Group>
         </children>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@../css/savedGames.css" />
      <URL value="@../css/common.css" />
   </stylesheets>
</VBox>
